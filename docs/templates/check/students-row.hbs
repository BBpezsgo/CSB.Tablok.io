<tr id="students-{{ID}}" style="display:none">
    <td colspan="8">
        {{#if Students}}
        <ul>
            {{#each Students}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        {{else}}
        {{#if Groups}}
        {{#each Groups}}
        <b>GROUP</b>
        <ul>
            {{#each Students}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        {{/each}}
        {{/if}}
        {{/if}}
    </td>
</tr>